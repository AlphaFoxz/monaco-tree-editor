{
  "name": "monaco-tree-editor",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "description": "the web ide with file tree based on 'monaco-editor'",
  "private": false,
  "keywords": [
    "moanco-editor",
    "vue-monaco-editor",
    "vue-monaco-tree",
    "web-editor"
  ],
  "license": "Apache-2.0",
  "author": {
    "name": "AlphaFoxz",
    "url": "https://github.com/AlphaFoxz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlphaFoxz/monaco-tree-editor"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm build-only && pnpm type-check && pnpm copy-file",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.vitest.json --composite false",
    "copy-file": "ncp README.md dist/README.md && ncp LICENSE dist/LICENSE && ncp package.publish.json dist/package.json",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "nanoid": "^5.0.4",
    "onigasm": "^2.2.5",
    "vue": "^3.3.13"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "monaco-editor": "^0.44.0",
    "ncp": "^2.0.0",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.26"
  }
}
