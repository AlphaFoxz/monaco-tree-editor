{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "lib/*",
    "lib/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": false,
    "emitDeclarationOnly": true,
    "declaration": true,
    "declarationDir": "dist",
    "types": [
      "node"
    ],
  }
}